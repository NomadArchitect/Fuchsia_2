{
    children: [
        {
            name: "stash",
            url: "fuchsia-pkg://fuchsia.com/dhcp-tests#meta/stash_secure.cm",
        },
        {
            name: "test",
            url: "fuchsia-pkg://fuchsia.com/dhcp-tests#meta/dhcp_lib_test_exec.cm",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: "#stash",
        },
        {
            storage: "data",
            from: "parent",
            to: "#stash",
        },
        {
            protocol: "fuchsia.stash.SecureStore",
            from: "#stash",
            to: "#test",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "#test",
        },
    ],
}
