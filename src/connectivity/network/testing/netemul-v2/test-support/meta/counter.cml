{
    include: [ "sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/counter",
    },
    capabilities: [
        { protocol: "fuchsia.netemul.test.Counter" },
    ],
    expose: [
        {
            protocol: "fuchsia.netemul.test.Counter",
            from: "self",
        },
    ],
}
