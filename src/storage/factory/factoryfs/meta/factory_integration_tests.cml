{
    include: [
        "//src/sys/test_runners/gtest/default.shard.cml",
        "inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "test/integration_test",
    },
    children: [
        {
            name: "isolated-devmgr",
            url: "fuchsia-pkg://fuchsia.com/storage_driver_test_realm#meta/storage_driver_test_realm.cm",
            startup: "eager",
        },
    ],
    use: [
        {
            protocol: [
                "fuchsia.process.Launcher",
                "fuchsia.sys.Launcher",
            ],
        },
        {
            directory: "deprecated-tmp",
            rights: [
                "admin",
                "rw*",
            ],
            path: "/tmp",
        },
        {
            directory: "dev",
            from: "#isolated-devmgr",
            rights: [ "r*" ],
            path: "/dev",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
                "fuchsia.sys.Launcher",
            ],
            from: "parent",
            to: "#isolated-devmgr",
        },
    ],
}
