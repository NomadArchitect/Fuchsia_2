{
    include: [
        "//src/storage/testing/driver_test_realm/meta/client.shard.cml",
        "//src/sys/test_runners/gtest/default.shard.cml",
        "inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "test/blobfs_component_test",
    },
    children: [
        {
            name: "blobfs",
            url: "fuchsia-boot:///#meta/blobfs.cm",
            startup: "eager",
        },
    ],
    use: [
        { protocol: "fuchsia.process.Launcher" },
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
        {
            protocol: [
                "fuchsia.fs.Admin",
                "fuchsia.fs.Query",
                "fuchsia.fs.startup.Startup",
            ],
            from: "#blobfs",
        },
        {
            directory: "blob-exec",
            from: "#blobfs",
            rights: [
                "execute",
                "rw*",
            ],
            path: "/blob_exec",
        },
        {
            directory: "blob",
            from: "#blobfs",
            rights: [ "rw*" ],
            path: "/blob",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.boot.WriteOnlyLog",
                "fuchsia.logger.LogSink",
            ],
            from: "parent",
            to: [ "#blobfs" ],
        },
        {
            directory: "dev",
            from: "#storage_driver_test_realm",
            to: [ "#blobfs" ],
        },
    ],
}
