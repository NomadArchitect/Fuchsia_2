{
    children: [
        {
            name: "starnix_runner",
            url: "fuchsia-pkg://fuchsia.com/starnix#meta/starnix_runner.cm",
        },
        {
            name: "hello_starnix",
            url: "fuchsia-pkg://fuchsia.com/hello_starnix#meta/hello_starnix.cm",
            startup: "eager",
            environment: "#starnix-env",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: "#starnix_runner",
        },
    ],
    expose: [
        {
            runner: "starnix",
            from: "#starnix_runner",
        },
    ],
    environments: [
        {
            name: "starnix-env",
            extends: "realm",
            runners: [
                {
                    runner: "starnix",
                    from: "#starnix_runner",
                },
            ],
        },
    ],
}
