{
    program: {
        runner: "starnix",
        binary: "system/bin/sh",
        args: [ "-i" ],
        mounts: [
            "/:ext4:data/system.img",
            "/data:remotefs:data",
            "/data/tmp:tmpfs",
            "/dev:devtmpfs",
            "/dev/pts:devpts",
            "/proc:proc",
        ],
        apex_hack: [ "com.android.runtime" ],
    },
}
