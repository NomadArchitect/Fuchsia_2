{
    children: [
        {
            name: "factory_driver_test_realm",
            url: "#meta/factory_driver_test_realm.cm",
            startup: "eager",
        },
    ],
    use: [
        {
            directory: "dev",
            from: "#factory_driver_test_realm",
            rights: [ "rw*" ],
            path: "/dev",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
                "fuchsia.sys.Launcher",
            ],
            from: "parent",
            to: [ "#factory_driver_test_realm" ],
        },
    ],
}
