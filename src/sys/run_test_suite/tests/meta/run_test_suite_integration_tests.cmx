{
    "facets": {
        "fuchsia.test": {
            "system-services": [
                "fuchsia.test.manager.RunBuilder"
            ]
        }
    },
    "include": [
        "//sdk/lib/diagnostics/syslog/client.shard.cmx"
    ],
    "program": {
        "binary": "bin/run_test_suite_integration_tests"
    },
    "sandbox": {
        "features": [
            "isolated-temp"
        ],
        "services": [
            "fuchsia.sys.Launcher",
            "fuchsia.test.manager.Query",
            "fuchsia.test.manager.RunBuilder"
        ]
    }
}