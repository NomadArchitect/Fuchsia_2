{
    include: [
        "sdk/lib/diagnostics/syslog/client.shard.cml",
        "src/sys/test_runners/rust/default.shard.cml",
    ],
    program: {
        binary: "bin/dynamic_child_stressor",
    },
    collections: [
        {
            name: "children",
            durability: "transient",
        },
    ],
    use: [
        {
            directory: "hub",
            from: "framework",
            rights: [ "r*" ],
            path: "/hub",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: [ "#children" ],
        },
    ],
    expose: [
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
    ],
}
