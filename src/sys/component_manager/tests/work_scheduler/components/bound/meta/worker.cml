{
    include: [ "sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/bound_worker",
    },
    capabilities: [
        { protocol: "fuchsia.sys2.Worker" },
    ],
    use: [
        { protocol: "fuchsia.sys2.WorkSchedulerControl" },
        {
            protocol: [ "fuchsia.sys2.WorkScheduler" ],
            from: "framework",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.sys2.Worker",
            from: "self",
            to: "framework",
        },
    ],
}
