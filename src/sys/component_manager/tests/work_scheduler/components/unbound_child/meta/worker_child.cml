{
    include: [ "//sdk/lib/diagnostics/syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/unbound_child_worker_child",
    },
    capabilities: [
        { protocol: "fuchsia.sys2.Worker" },
    ],
    expose: [
        {
            protocol: "fuchsia.sys2.Worker",
            from: "self",
            to: "framework",
        },
        {
            protocol: "fuchsia.sys2.WorkScheduler",
            from: "framework",
        },
    ],
}
