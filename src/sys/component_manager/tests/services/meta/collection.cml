{
    collections: [
        {
            name: "coll",
            durability: "transient",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: [ "#coll" ],
        },
    ],
    expose: [
        {
            service: "fuchsia.test.services.Service",
            from: "#coll",
        },
        {
            protocol: "fuchsia.sys2.Realm",
            from: "framework",
        },
    ],
}
