library fuchsia.test.services;

/// A simple Service that reports the name of its instance.
service Service {
    InstanceReporter instance_reporter;
};

/// Reports the name of its encapsulating Service's instance.
protocol InstanceReporter {
    ReportInstance() -> (string:MAX instance);
};
