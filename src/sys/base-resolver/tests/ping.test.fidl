deprecated_syntax;

library test.ping;

alias PingString = string:100;
alias PongString = string:105;

[Discoverable]
protocol Ping {
    /// Returns a string that is the concatenation of the input `ping`
    /// a space and the string "pong".
    Ping(PingString ping) -> (PongString pong);
};
