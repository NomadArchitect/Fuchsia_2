success("FlexibleBitsUint8KnownValue") {
    // Not yet supported.
    bindings_denylist = [cpp],
    value = FlexibleBitsUint8Struct{
        b: 1,
    },
    bytes = {
        v1, v2 = [
            0x01, padding:7,
        ],
    },
}

