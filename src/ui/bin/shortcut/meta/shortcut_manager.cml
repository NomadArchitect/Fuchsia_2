{
    include: [
        // Enable system logging.
        "syslog/client.shard.cml",
    ],
    program: {
        runner: "elf",
        binary: "bin/shortcut",
    },
    capabilities: [
        {
            protocol: [
                "fuchsia.ui.shortcut.Manager",
                "fuchsia.ui.shortcut.Registry",
            ],
        },
    ],
    expose: [
        {
            protocol: [
                "fuchsia.ui.shortcut.Manager",
                "fuchsia.ui.shortcut.Registry",
            ],
            from: "self",
        },
    ],
}
