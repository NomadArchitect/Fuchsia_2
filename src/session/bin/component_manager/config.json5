{
    debug: false,
    maintain_utc_clock: false,
    use_builtin_process_launcher: false,
    builtin_pkg_resolver: "appmgr_bridge",
    out_dir_contents: "svc",
    num_threads: 2,
    // See session_manager.cml for comments.
    namespace_capabilities: [
        {
            protocol: [
                "fuchsia.cobalt.LoggerFactory",
                "fuchsia.logger.LogSink",
                "fuchsia.sys.Launcher",
                "fuchsia.sys.Loader",
                "fuchsia.tracing.provider.Registry",
                "fuchsia.ui.focus.FocusChainListenerRegistry",
                "fuchsia.ui.input3.Keyboard",
                "fuchsia.ui.input.ImeService",
                "fuchsia.ui.scenic.Scenic",
                "fuchsia.ui.shortcut.Manager",
                "fuchsia.ui.views.ViewRefInstalled"
            ],
        },
        {
            directory: "config-build-info",
            path: "/config/build-info",
            rights: [ "r*" ],
        },
        {
            directory: "config-ssl",
            path: "/config/ssl",
            rights: [ "r*" ],
        },
        {
            directory: "data",
            path: "/data",
            rights: [ "rw*" ],
        },
        {
            directory: "dev-input",
            path: "/dev/class/input",
            rights: [ "r*" ],
        },
        {
            directory: "dev-input-report",
            path: "/dev/class/input-report",
            rights: [ "r*" ],
        },
        {
            directory: "dev-null",
            path: "/dev/null",
            rights: [ "rw*" ],
        },
        {
            directory: "dev-zero",
            path: "/dev/zero",
            rights: [ "rw*" ],
        },
        {
            directory: "config-data",
            path: "/pkgfs/packages/config-data/0/meta/data",
            rights: [ "r*" ],
        },
        {
            directory: "tmp",
            path: "/tmp",
            rights: [ "rw*" ],
        },
    ],
    builtin_capabilities: [
        {
            protocol: [
                "fuchsia.sessionmanager.Startup",
                "fuchsia.sys2.CrashIntrospect",
                "fuchsia.sys2.EventSource",
                "fuchsia.sys2.SystemController",
                "fuchsia.sys2.WorkSchedulerControl",
            ],
        },
    ],
}
