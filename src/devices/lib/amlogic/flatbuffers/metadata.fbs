namespace Amlogic.Metadata;

table OperatingPoint {
    frequency: uint;
    voltage: uint;
}

table AmlogicCpuPerformanceDomain {
    id: uint;
    core_count: uint;
    relative_performance: ubyte;
    name: string;
    operating_points: [OperatingPoint];
}

table AmlogicCpuMetadata {
    domains: [AmlogicCpuPerformanceDomain];
}

root_type AmlogicCpuMetadata;