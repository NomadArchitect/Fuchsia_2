composite leaf;

using fuchsia.devicegroupbind.test;

primary node "left" {
  fuchsia.devicegroupbind.test.TEST_BIND_PROPERTY ==
    fuchsia.devicegroupbind.test.TEST_BIND_PROPERTY.DRIVER_LEFT;
}

node "right" {
  fuchsia.devicegroupbind.test.TEST_BIND_PROPERTY ==
    fuchsia.devicegroupbind.test.TEST_BIND_PROPERTY.DRIVER_RIGHT;
}

optional node "opt" {
  fuchsia.devicegroupbind.test.TEST_BIND_PROPERTY ==
    fuchsia.devicegroupbind.test.TEST_BIND_PROPERTY.DRIVER_OPTIONAL;
}
