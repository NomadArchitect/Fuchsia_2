library fuchsia.lifecycle.test;

@discoverable
protocol Device {
    Ping() -> ();
};

service Service {
    device client_end:Device;
};

@transport("Banjo")
@banjo_layout("ddk-protocol")
protocol Parent {
    GetString() -> (struct {
        response string:MAX;
    });
};
