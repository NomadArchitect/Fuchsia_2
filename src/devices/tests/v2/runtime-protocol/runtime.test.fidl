library fuchsia.runtime.test;

using zx;

@discoverable
@transport("Driver")
protocol Setter {
    Set(struct {
        value uint32;
    }) -> () error zx.status;
};

@discoverable
@transport("Driver")
protocol Getter {
    Get() -> (struct {
        value uint32;
    }) error zx.status;
};

service Service {
    setter client_end:Setter;
    getter client_end:Getter;
};

@discoverable
protocol Waiter {
    Ack();
};
