library fidl.rust.test.external;

using zx;

protocol SendHandleProtocol {
    SendHandle(zx.handle:<EVENT, zx.rights.TRANSFER> h);
};

protocol EchoHandleProtocol {
    EchoHandle(zx.handle:<EVENT, zx.rights.TRANSFER | zx.rights.DUPLICATE> h) -> (zx.handle:<EVENT, zx.rights.TRANSFER> h);
};

protocol PushEventProtocol {
    -> PushEvent(zx.handle:<EVENT, zx.rights.TRANSFER> h);
};
