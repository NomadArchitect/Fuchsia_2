{
    "facets": {
        "fuchsia.test": {
            "system-services": [
                "fuchsia.kernel.HypervisorResource",
                "fuchsia.kernel.VmexResource",
                "fuchsia.sysinfo.SysInfo"
            ]
        }
    },
    "include": [
        "//sdk/lib/diagnostics/syslog/client.shard.cmx"
    ],
    "program": {
        "binary": "bin/virtualization_block_tests"
    },
    "sandbox": {
        "features": [
            "isolated-temp"
        ],
        "services": [
            "fuchsia.kernel.HypervisorResource",
            "fuchsia.kernel.VmexResource",
            "fuchsia.sys.Environment",
            "fuchsia.sys.Loader",
            "fuchsia.sysinfo.SysInfo"
        ]
    }
}
