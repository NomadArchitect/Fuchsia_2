source_set("pthread") {
  configs += [ "//build/unification/config:zircon-migrated" ]

  testonly = true
  sources = [
    "pthread.cc",
    "pthread_c.c",
  ]

  cflags_c = [ "-Wgnu-empty-initializer" ]
  deps = [
    "//sdk/lib/fdio",
    "//zircon/system/ulib/zxtest",
  ]
}
