library zx;

[Transport = "Syscall"]
protocol Categories {
    [Noreturn]
    NoResponseNoReturn(int32 input);

    [blocking]
    Looper(int32 a) -> (int32 b);

    [vdsocall]
    Vdsoer(int32 a) -> (int32 b);

    [const, Vdsocall]
    TwoCategories(int32 a) -> (int32 b);
};
