library fidl.difl.test;

// After

//!DeclAdded
table TableAdded {
    1: int32 field;
};

// Attributes

[Attribute]
table TableAttributeAdded {
    1: int32 field;
};

table TableAttributeRemoved {
    1: int32 field;
};

[Attribute="after"]
table TableAttributeChanged {
    1: int32 field;
};

// Members

table TableMemberAdded {
    1: int32 one;
