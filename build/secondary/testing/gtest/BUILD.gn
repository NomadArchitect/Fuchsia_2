# Alias for GTest GN targets, used by Perfetto.

group("gtest") {
  testonly = true
  deps = [ "//third_party/googletest:gtest" ]
}

group("gtest_main") {
  testonly = true
  deps = [ "//third_party/googletest:gtest_main" ]
}
