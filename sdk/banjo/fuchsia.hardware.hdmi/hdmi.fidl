library fuchsia.hardware.hdmi;
using zx;

[Transport = "Banjo", BanjoLayout = "ddk-protocol"]
protocol Hdmi {
    /// Pass a channel handle to the hdmi driver to connect to its
    /// FIDL interface.
    Connect(zx.handle:CHANNEL server_chan) -> ();
};
