library fuchsia.systemmonitor;

using zx;

[Discoverable]
protocol Harvester {
    /// Connect a socket over which the legacy gRPC protocol can be used.
    ConnectGrpc(zx.handle:SOCKET socket) -> () error zx.status;
};
