library fuchsia.tpm;

using zx;

protocol TpmDevice {
    /// Get the TPM's device ID, vendor ID, and revision ID.
    GetDeviceId() -> (struct {
        vendor_id uint16;
        device_id uint16;
        revision_id uint8;
    }) error zx.status;
};
