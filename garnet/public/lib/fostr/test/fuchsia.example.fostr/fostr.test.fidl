deprecated_syntax;
library fuchsia.example.fostr;

bits ExampleBits {
    A = 1;
    B = 2;
    C = 4;
};

enum ExampleEnum {
    FOO = 1;
    BAR_BAZ = 2;
};

struct MyStruct {
    vector<int32>:10? nums;
    string foo;
    MyXunion bar;
    uint8 my_uint8;
    int8 my_int8;
};

flexible union MyXunion {
    1: int32 i;
    2: bool b;
    3: reserved;
    4: uint8 my_uint8;
    5: int8 my_int8;
};

union MyUnion {
    1: int32 i;
    2: bool b;
    3: reserved;
    4: uint8 my_uint8;
    5: int8 my_int8;
};

table SimpleTable {
    1: bool x;
    2: reserved;
    3: MyStruct y;
    4: int32 z;
    5: uint8 my_uint8;
    6: int8 my_int8;
};
