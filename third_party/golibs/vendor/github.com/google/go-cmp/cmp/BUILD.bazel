load("@io_bazel_rules_go//go:def.bzl", "go_library", "go_test")

package(default_visibility = ["//visibility:public"])

go_library(
    name = "cmp",
    srcs = [
        "compare.go",
        "export_panic.go",
        "export_unsafe.go",
        "options.go",
        "path.go",
        "report.go",
        "report_compare.go",
        "report_references.go",
        "report_reflect.go",
        "report_slices.go",
        "report_text.go",
        "report_value.go",
    ],
    importpath = "github.com/google/go-cmp/cmp",
    deps = [
        "//third_party/golibs/vendor/github.com/google/go-cmp/cmp/internal/diff",
        "//third_party/golibs/vendor/github.com/google/go-cmp/cmp/internal/flags",
        "//third_party/golibs/vendor/github.com/google/go-cmp/cmp/internal/function",
        "//third_party/golibs/vendor/github.com/google/go-cmp/cmp/internal/value",
    ],
)
